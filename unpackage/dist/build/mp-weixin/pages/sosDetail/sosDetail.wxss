.sos-detail map{height:50vh;width:750rpx}.sos-detail .panel{position:fixed;bottom:0;padding:30rpx;box-sizing:border-box;left:0;right:0;margin:0 auto;background:#fff;box-shadow:0 6rpx 18rpx 0 rgba(36,35,34,.11);border-radius:20rpx 20rpx 0 0}.sos-detail .panel .title{position:relative;margin-bottom:37rpx}.sos-detail .panel .title text{font-size:36rpx;font-weight:700;color:#333231}.sos-detail .panel .title .navigation{position:absolute;right:0;top:0;width:100rpx;height:100rpx;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;-webkit-box-align:center;-webkit-align-items:center;align-items:center;background:#ffcb3d;border-radius:50%;box-shadow:0 8rpx 16rpx 0 rgba(158,116,1,.18)}.sos-detail .panel .title .navigation image{width:60%;height:60%}.sos-detail .panel .content{font-size:30rpx;font-weight:500;color:#999895}.sos-detail .panel .content .dl{display:-webkit-box;display:-webkit-flex;display:flex;margin-bottom:12rpx}.sos-detail .panel .content .dl .dt{-webkit-flex-shrink:0;flex-shrink:0}.sos-detail .panel .btn{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-align:center;-webkit-align-items:center;align-items:center;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;width:100%;height:96rpx;background:#ffcb3d;box-shadow:0 8rpx 16rpx 0 rgba(158,115,1,.18);border-radius:10rpx;font-size:34rpx;font-weight:400;color:#333231;margin-top:40rpx}.sos-detail .panel .disabled{background:#b3b1af!important;box-shadow:none!important}